# API Configuration
REACT_APP_API_URL=http://localhost:8000/api
REACT_APP_API_TIMEOUT=30000

# Application Configuration
REACT_APP_NAME=AI Agent Platform
REACT_APP_VERSION=1.0.0
REACT_APP_DESCRIPTION=A platform for managing AI agents and workflows

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_THEMING=true
REACT_APP_ENABLE_I18N=true

# Authentication Configuration
REACT_APP_AUTH_TOKEN_EXPIRY=3600
REACT_APP_AUTH_REFRESH_TOKEN_EXPIRY=604800

# File Upload Configuration
REACT_APP_MAX_FILE_SIZE=10485760
REACT_APP_ALLOWED_FILE_TYPES=text/plain,text/csv,application/json,application/xml,application/pdf,image/jpeg,image/png,image/gif

# Pagination Configuration
REACT_APP_DEFAULT_PAGE_SIZE=10
REACT_APP_PAGE_SIZE_OPTIONS=5,10,25,50,100

# Analytics Configuration
REACT_APP_ANALYTICS_ID=your-analytics-id
REACT_APP_ANALYTICS_ENABLED=true

# Error Tracking Configuration
REACT_APP_ERROR_TRACKING_ENABLED=true
REACT_APP_ERROR_TRACKING_DSN=your-error-tracking-dsn

# Performance Monitoring Configuration
REACT_APP_PERFORMANCE_MONITORING_ENABLED=true
REACT_APP_PERFORMANCE_MONITORING_SAMPLE_RATE=0.1

# Development Configuration
REACT_APP_ENABLE_DEV_TOOLS=true
REACT_APP_ENABLE_LOGGING=true
REACT_APP_LOG_LEVEL=debug

# Testing Configuration
REACT_APP_ENABLE_TESTING=true
REACT_APP_TEST_API_URL=http://localhost:8001/api

# Production Configuration
REACT_APP_ENABLE_CACHING=true
REACT_APP_CACHE_DURATION=3600
REACT_APP_ENABLE_COMPRESSION=true
REACT_APP_ENABLE_CDN=true
REACT_APP_CDN_URL=https://cdn.your-domain.com 